{
  "name": "Scrap",
  "description": "Scrapping the pages",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "debugger"
  ],
  "browser_action": {
    "default_title": "Scrap the page",
    "default_popup": "scrap_main.html"
  },
  "options_ui" : {
    "page": "options.html",
    "open_in_tab": true
  },
  "manifest_version": 2
}
